.pocketMoneyBox {
  width: 100%;
  height: 60px;
  background: #f6f6fb;
  line-height: 60px;
}

.pocketMoneyBox image {
  width: 30px;
  height: 30px;
  margin-top: 15px;
  margin-left: 10px;
}

.pocketmoney1 {
  margin-right: 187px;
  float: right;
}

.pocketmoney2 {
  margin-right: 10px;
  float: right;
  color: red;
}

.bisicard {
  width: 93%;
  margin: 0 auto;
  height: 110px;
  position: relative;
  top: 0;
  left: 0;
  border-radius: 10px;
  margin-bottom: 10px;
  margin-top: 5px;
}

.bisicard image {
  width: 100%;
  height: 110px;
  margin: 0 auto;
  border-radius: 5px;
}

.abloca image {
  /* position: absolute; */
  width: 52px;
  height: 52px;
  margin-top: 4px;
  margin-left: 4px;
}

.ablocaimg {
  position: absolute;
  width: 60px;
  height: 60px;
  border-radius: 30px;
  top: 10px;
  left: 10px;
  background: #fff;
}

.bisicardtop, .bisicardbot, .bisicardcardnum {
  position: absolute;
  color: #fff;
  letter-spacing: 2px;
}

.bisicardtop {
  top: 20px;
  left: 80px;
  font-size: 16px;
  font-weight: bold;
}

.bisicardbot {
  top: 48px;
  left: 80px;
  font-size: 13px;
}

.bisicardcardnum {
  top: 80px;
  left: 85px;
  font-size: 18px;
  font-weight: bold;
}

.addbusicards {
  width: 94%;
  height: 60px;
  background: #eee;
  color: gray;
  text-align: center;
  line-height: 60px;
  margin: 0 auto;
  margin-top: 20px;
  border-radius: 5px;
}
